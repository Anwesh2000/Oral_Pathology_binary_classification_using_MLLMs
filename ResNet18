tch_size:\n","                yield (\n","                    batch_img_path,\n","                    batch_label_name,\n","                    torch.stack(batch_img),\n","                    torch.tensor(batch_labels, dtype=torch.float),\n","\n","                )\n","                batch_img, batch_labels,batch_img_path,batch_label_name = [], [], [], []\n","\n","        # last partial batch\n","        if batch_img:\n","            yield(\n","                    batch_img_path,\n","                    batch_label_name,\n","                    torch.stack(batch_img),\n","                    torch.tensor(batch_labels, dtype=torch.float),\n","                )"],"metadata":{"id":"3He1D7KzFjZh"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["train_gen = BatchGenerator(train_samples, batch_size=32)\n","val_gen = BatchGenerator(val_samples, batch_size=32)\n","test_gen = BatchGenerator(test_samples, batch_size=32)"],"metadata":{"id":"eBc0-HstqlnJ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["len(train_gen),len(val_gen),len(test_gen)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"3Xzr9ERfiEGn","executionInfo":{"status":"ok","timestamp":1748514663383,"user_tz":-330,"elapsed":14,"user":{"displayName":"Anwesh Nayak","userId":"10812060260948452341"}},"outputId":"85a6905a-4dcf-4822-8b51-8a26f70b5f39"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["(19, 5, 10)"]},"metadata":{},"execution_count":171}]},{"cell_type":"code","source":["for pth,label_name,imgs, labels in tqdm(train_gen, desc='Train', leave=False):\n","        imgs, labels = imgs.to(device), labels.to(device)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"goOkC-W3JbKi","executionInfo":{"status":"ok","timestamp":1748514663436,"user_tz":-330,"elapsed":49,"user":{"displayName":"Anwesh Nayak","userId":"10812060260948452341"}},"outputId":"064f087e-d832-4bd1-db44-694752a6c90f"},"execution_count":n